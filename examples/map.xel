entry main() {
    let dummy: map = map()
    dummy.put("hello", "world")
    if dummy.has("hello") {
        let value = dummy.get("hello")
        println("Value: " + value)
    }

    let len = dummy.len()
    println("Dummy size: " + len)
    dummy.remove("hello")
    len = dummy.len()
    println("Dummy size after remove: " + len)

    return 0
}